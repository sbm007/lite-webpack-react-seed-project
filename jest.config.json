{
  "testEnvironment": "node",
  "moduleNameMapper": {
    "~/(.*)$": "<rootDir>/src/app/$1",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/misc/fileMock.js",
    "\\.(css|scss)$": "<rootDir>/misc/styleMock.js"
  },
  "testRegex": ".*\\.spec\\.js$",
  "setupTestFrameworkScriptFile": "<rootDir>/misc/setupTests.js"
}
